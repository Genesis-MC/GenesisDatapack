data modify storage gen:guidebook chapters append value {id:"genesis:book/book_of_stats",name:"Book of Stats",actions:[],pages:['["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Physical Power\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.physical_power.1","with":[0]}]},{"text":"\\n\\nAffects the amount of damage you deal! Every 10 points is equal to 1 attack damage."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Attack Speed\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.attack_speed.1","with":[0]}]},{"text":"\\n\\nAffects the rate at which you attack! The specific equation is (A*20)/(1000+A) where A is Attack Speed."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Health\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.health.1","with":[0]}]},{"text":"\\n\\nAffects your maximum health! Every 10 points is equal to 1 extra health, or half a heart."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Armor\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.armor.1","with":[0]}]},{"text":"\\n\\nAffects the amount of armor you have, which decreases the damage you take! Every 10 points is equal to 1 point of vanilla armor."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Armor Toughness\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.armor_toughness.1","with":[0]}]},{"text":"\\n\\nAffects the effectiveness of armor to further decrease the damage you take! Every 10 points is equal to 1 point of vanilla armor toughness."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Speed\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.speed.1","with":[0]}]},{"text":"\\n\\nAffects how fast you move!"}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Knockback Resistance\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.knockback_resistance.1","with":[0]}]},{"text":"\\n\\nAffects the amount of knockback you take! Each point corresponds to 1% of knockback resistance."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Magic Power\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.magic_power.1","with":[0]}]},{"text":"\\n\\nAffects the potency of certain abilities which scale off of magic power!"}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Mana Pool\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.mana_pool.1","with":[0]}]},{"text":"\\n\\nAffects your maximum amount of mana! Each point equals 1 extra maximum mana."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Mana Regen\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.mana_regen.1","with":[0]}]},{"text":"\\n\\nAffects the amount of mana regenerated! Every 5 points is equal to 1 extra mana per second."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Ability Haste\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.ability_haste.1","with":[0]}]},{"text":"\\n\\nAffects the cooldown of your abilities! The specific equation is (C*100) / (H+100), where C is the original cooldown duration in ticks and H is ability haste."}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Luck\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.luck.1","with":[0]}]},{"text":"\\n\\nAffects your luck, which boosts loot drops from certain sources, such as treasures!"}]','["",{"storage":"gen:guidebook","nbt":"macro.navbar","interpret":true},{"text":"Relic Power\\n","bold":true},{"translate":"","font":"genesis:stats","color":"#FFFFE6","italic":false,"extra":[{"translate":"genesis.stats.wrapper.relic_power.1","with":[0]}]},{"text":"\\n\\nNot yet implemented! The relic system will come in a future update."}]']}
